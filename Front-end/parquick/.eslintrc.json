{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "flowtype"],
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "react/prop-types": "off"
  }
}
